   "source": [
    "# busca binaria dividir e conquistar\n",
    "\n",
    "def busca_binaria(array, x, low, high):\n",
    "    while low <= high:\n",
    "        mid = (low + high) // 2\n",
    "        if array[mid] == x:\n",
    "            return mid\n",
    "        elif array[mid] < x:\n",
    "            low = mid + 1"
   ]
